{
  "name": "crown-mural-interactive",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run process:image && vite build",
    "build:vite-only": "vite build",
    "pip:install": "python -m pip install -r requirements.txt",
    "preview": "vite preview --open",
    "process:image": "python scripts/regions.py public/data/Mural_Crown_of_Italian_City.svg.png --blur 1 --close 1 --min_area 5 --canny_sigma 0.25 --mask_mode all --palette_mode kbatch --sample_step 7 --masks_dir shape_masks",
    "deploy": "npm run build"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0",
    "vite": "^5.4.11"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "overrides": {
    "esbuild": "^0.25.0"
  }
}
